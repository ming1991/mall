package com.itheima31.jdmall.bean;

import java.io.Serializable;

/**
 * Created by Administrator on 2016/10/26.
 * Todo:
 */
public class ShopBean implements Serializable {
    public String user_id;
    public int Product_id;//商品id
    public int color;
    public int size;
    public int num;

    public ShopBean(){}

    public ShopBean(String user_id, int product_id, int num,int color, int size) {
        this.user_id = user_id;
        Product_id = product_id;
        this.color = color;
        this.size = size;
        this.num = num;
    }

    @Override
    public String toString() {
        return ""+Product_id+","+color+","+size;
    }
}
